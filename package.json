{
  "name": "FunInATL",
  "version": "1.0.0",
  "description": "A full list of events going on in Atlanta",
  "author": "Charlie Page",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "staging": "cp .env.staging .env && nuxt-generate -b -v 4 --fail-on-page-error",
    "production": "cp .env.production .env && nuxt-generate -b -v 4 --fail-on-page-error",
    "local": "cp .env.local .env && nuxt-generate -b -v 4 --fail-on-page-error",
    "copy": "cp -r dist/* public/",
    "deploy": "mkdir dist && cp -r public/* dist/"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.0-rc19",
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/recaptcha": "^0.6.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/universal-storage": "^0.5.4",
    "bulmaswatch": "^0.8.0",
    "cross-env": "^7.0.2",
    "eslint-plugin-html": "^6.0.0",
    "graphql-tag": "^2.10.3",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "nuxt": "^2.12.0",
    "nuxt-buefy": "^0.3.24",
    "nuxt-generate-cluster": "^2.7.0",
    "nuxt-mq": "^2.0.1",
    "nuxt-payload-extractor": "^1.0.0",
    "sass-loader": "^8.0.2",
    "vanilla-back-to-top": "^7.2.1",
    "vue": "2.6.11",
    "vue-clazy-load": "^0.4.2",
    "vue-server-renderer": "2.6.11",
    "vue-template-compiler": "2.6.11",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nuxtjs/eslint-config": "^2.0.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": ">=14.1.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": ">=2.20.1",
    "eslint-plugin-jest": ">=23.8.2",
    "eslint-plugin-node": ">=11.0.0",
    "eslint-plugin-nuxt": ">=0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.1"
  }
